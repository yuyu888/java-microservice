<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.myproject.models.testservice.dao" >
  <resultMap id="BaseResultMap" type="com.myproject.models.testservice.entity.LogEntity" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="uid" property="uid" jdbcType="INTEGER" />
    <result column="operate_type" property="operateType" jdbcType="INTEGER" />
    <result column="opeate_detail" property="opeateDetail" jdbcType="INTEGER" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
  </resultMap>
  <insert id="insertBatch" parameterType="java.util.List">
    insert into log (uid,
    operate_type, opeate_detail
    )
    values
    <foreach collection="datalist" item="item" separator=",">
      (#{item.uid,jdbcType=INTEGER},
      #{item.operate_type,jdbcType=INTEGER},
      #{item.opeate_detail,jdbcType=VARCHAR},
    </foreach>
  </insert>
  <select id="getListByIds" resultType="com.myproject.models.testservice.entity.LogEntity">
    SELECT * FROM log WHERE 1 = 1
    <if test="null != ids and ids.size > 0">
      AND id IN
      <foreach item="item" index="index" collection="ids" open="(" separator="," close=")">
        #{item}
      </foreach>
    </if>
  </select>
</mapper>